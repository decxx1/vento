{
  "$schema": "../gen/schemas/desktop-capability.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:webview:default",
    "opener:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select"
  ]
}
